&star_job

read_extra_star_job_inlist(1) = .true.
extra_star_job_inlist_name(1) = 'inlist_both'

read_extra_star_job_inlist(2) = .true.
extra_star_job_inlist_name(2) = 'inlist_extra'

save_model_filename = 'MASS1donor_final.mod'

change_rotation_flag = .false.
change_initial_rotation_flag = .true.
new_rotation_flag = .false.
new_omega_div_omega_crit = 0
set_initial_omega_div_omega_crit = .true.
set_omega_div_omega_crit = .false.

pgstar_flag = .true.

/ ! end of star_job namelist

